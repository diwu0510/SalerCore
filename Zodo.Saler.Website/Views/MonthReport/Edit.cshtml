@model Zodo.Saler.Services.MonthReportEntity

@{
    ViewData["Title"] = "MonthReport编辑";
}

<form asp-action="Edit">
    <input type="hidden" asp-for="Id">
    <h1 class="form-box-title">@ViewData["Title"]</h1>
    <hr />
    <div class="form-box-fixed-container">
        <div class="form-box">
            <div class="row">
                <div class="col-6">
                    <div class="label">销售ID</div>
                    <div class="control">
                        <select asp-for="SalerId" asp-items="ViewBag.Salers" isvalid="yes" checkexpession="NotNull">
                            <option value="">请选择</option>
                        </select>
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">部门ID</div>
                    <div class="control">
                        <select asp-for="DeptId" asp-items="ViewBag.Depts" isvalid="yes" checkexpession="NotNull">
                            <option value="">请选择</option>
                        </select>
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">年度</div>
                    <div class="control">
                        <input type="number" asp-for="Year" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">月份</div>
                    <div class="control">
                        <input type="number" asp-for="Month" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">飞机</div>
                    <div class="control">
                        <input type="number" asp-for="FJ" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">火车</div>
                    <div class="control">
                        <input type="number" asp-for="HC" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">巴士</div>
                    <div class="control">
                        <input type="number" asp-for="BS" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">自驾车</div>
                    <div class="control">
                        <input type="number" asp-for="ZJC" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">的士费</div>
                    <div class="control">
                        <input type="number" asp-for="DSF" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">业务招待费</div>
                    <div class="control">
                        <input type="number" asp-for="YWZDF" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">住宿费</div>
                    <div class="control">
                        <input type="number" asp-for="ZSF" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">住宿节约奖励</div>
                    <div class="control">
                        <input type="number" asp-for="ZSJYJL" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">交通津贴</div>
                    <div class="control">
                        <input type="number" asp-for="JTJT" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">餐费津贴</div>
                    <div class="control">
                        <input type="number" asp-for="CFJT" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">通讯费</div>
                    <div class="control">
                        <input type="number" asp-for="TXF" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">其他费用</div>
                    <div class="control">
                        <input type="number" asp-for="QTFY" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">薪资</div>
                    <div class="control">
                        <input type="number" asp-for="XZ" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">业绩</div>
                    <div class="control">
                        <input type="number" asp-for="YJ" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">服务费</div>
                    <div class="control">
                        <input type="number" asp-for="FWF" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">账面毛利</div>
                    <div class="control">
                        <input type="number" asp-for="ZMML" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
                <div class="col-6">
                    <div class="label">毛利</div>
                    <div class="control">
                        <input type="number" asp-for="ML" isvalid="yes" checkexpession="Number" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="form-box-fixed-foot row">
        <div class="col-6">
            <label>&nbsp;</label>
        </div>
        <div class="col-6 content-right">
            <input type="submit" value="提交" class="btn btn-blue" />
            <input id="btn-closeSelf" type="button" value="关闭" class="btn btn-red btn-close" />
        </div>
    </div>
</form>

@section scripts {
    <script src="~/lib/validator/validator.js"></script>
    <script>
        myUI.initForm({pageType: "pop"});
    </script>
}
